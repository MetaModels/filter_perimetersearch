{
  "name": "metamodels/filter_perimetersearch",
  "description": "MetaModels perimeter search",
  "keywords": [
    "contao",
    "metamodels",
    "perimeter",
    "search",
    "filter"
  ],
  "type": "contao-module",
  "homepage": "http://now.metamodel.me/",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Christian Schiffler",
      "email": "c.schiffler@cyberspectrum.de",
      "homepage": "http://www.cyberspectrum.de",
      "role": "Developer"
    },
    {
      "name": "Stefan Heimes",
      "email": "stefan_heimes@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "mail@metamodel.me",
    "issues": "https://github.com/MetaModels/filter_perimetersearch/issues",
    "wiki": "http://de.contaowiki.org/MetaModels",
    "irc": "irc://irc.freenode.org/contao.mm",
    "source": "https://github.com/MetaModels/filter_perimetersearch"
  },
  "require": {
    "php": "^5.6 || ^7.0",
    "contao/core-bundle": "^4.4",
    "contao-community-alliance/dc-general": "~2.1@dev",
    "metamodels/core": "~2.1@dev",
    "menatwork/contao-multicolumnwizard": "^3.3",
    "symfony/dependency-injection": "^3.3"
  },
  "require-dev": {
    "cyberspectrum/contao-toolbox": "^0.7",
    "phpcq/all-tasks": "^1.2",
    "contao/manager-plugin": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "MetaModels\\FilterPerimetersearchBundle\\": "src"
    },
    "files": [
      "src/deprecated-autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "MetaModels\\FilterPerimetersearchBundle\\": "src"
    }
  },
  "extra": {
    "contao-manager-plugin": "MetaModels\\FilterPerimetersearchBundle\\ContaoManager\\Plugin",
    "contao": {
      "transifex": {
        "project": "metamodels",
        "prefix": "filter_perimetersearch-",
        "languages_cto": "contao/languages",
        "languages_tx": ".tx"
      }
    },
    "branch-alias": {
      "dev-master": "2.0.x-dev",
      "dev-develop": "2.1.x-dev"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@gitlab.com:metamodels/core.git"
    }
  ]
}
